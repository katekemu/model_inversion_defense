   "metadata": {},
   "outputs": [],
   "source": [
    "folder = \"./data/img_align_celeba/img_align_celeba\"\n",
    "for i in os.listdir(folder):\n",
    "    file = f\"{folder}/{i}\"\n",
    "    im = Image.open(file)\n",
    "    im = im.crop((35, 70, 35+108, 70+108))\n",
    "    im.save(f'./data/celeba_108/celeba_108/{i}')"
   ]
